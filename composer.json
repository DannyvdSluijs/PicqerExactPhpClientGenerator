{
    "name": "dannyvandersluijs/picqer-exact-php-client-generator",
    "description": "A generator for the picqer exact php client models",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3",
        "dannyvandersluijs/exact-online-meta-data-tool": "^4.0",
        "nikic/php-parser": "^5.3",
        "symfony/console": "^7.2",
        "symfony/dom-crawler": "^7.2",
        "symfony/string": "^7.2",
        "symfony/templating": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "PicqerExactPhpClientGenerator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "rector/rector": "^1.2"
    },
    "autoload-dev": {
        "psr-4": {
            "PicqerExactPhpClientGeneratorTest\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
