{
    "name": "dannyvandersluijs/picqer-exact-php-client-generator",
    "description": "A generator for the picqer exact php client models",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "^6.4",
        "php": "^8.3",
        "dannyvandersluijs/exact-online-meta-data-tool": "^3.0",
        "json-mapper/json-mapper": "^2.5",
        "symfony/templating": "^6.4",
        "symfony/string": "^6.4",
        "nikic/php-parser": "^4.10",
        "symfony/dom-crawler": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "PicqerExactPhpClientGenerator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^1.2"
    }
}
